{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "sourceMap": true,
        "declaration": false,
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "module": "Node16",
        "moduleResolution": "Node16",
        "rootDir": "web",
        "outDir": "web/dist",
        "target": "ES2020",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "ES2020",
            "dom"
        ]
    },
    "files": [
        "web/website/src/main.ts"
    ],
    "include": [
        "web/website/src/**/*.d.ts"
    ]
}